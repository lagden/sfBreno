.home-gallery
  visibility hidden
  overflow: hidden
  height 400px
  img
    display block
    height 400px

.home-gallery--done
  visibility visible

.static-banner--home
  position absolute
  bottom 0
  box-sizing border-box
  padding 20px
  margin 0
  color: $cores.white
  background: rgba($cores.black, 0.3)
  width 100%
  z-index 100
  pointer-events: none

.gallery__caption
  display none

.estate-gallery
  visibility hidden
  overflow: hidden
  height 200px
  img
    display block
    height 200px

.estate-gallery--done
  visibility visible
